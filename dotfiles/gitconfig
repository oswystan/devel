[alias]
  st = status -s
  ci = commit
  co = checkout
  br = branch -vv
  cl = clean -fd
  df = diff
  lg = log --name-status
	ls = ls-tree HEAD
	cat = cat-file -p
	ll = log --oneline
	llg = log --oneline --graph
  lv = log --pretty='%C(auto)%h %G? %ad %an - %d%s'
  c = clone --depth 1
